{
  "name": "nickyt-site",
  "type": "module",
  "version": "1.0.0",
  "description": "Nick Taylor's personal website built with Astro",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run format:check && astro check && astro build && pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write . && eslint --fix .",
    "format:check": "prettier --check . && eslint --max-warnings 0 .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
